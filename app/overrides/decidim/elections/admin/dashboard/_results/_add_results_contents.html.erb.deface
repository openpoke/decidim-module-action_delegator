<!-- replace "erb[loud]:contains('decidim/elections/admin/dashboard/questions_with_results')" -->

<% if settings_for(election).present? && lookup_context.exists?("decidim/action_delegator/admin/dashboard/#{params[:results]}", [], true) %>
  <%= render "decidim/action_delegator/admin/dashboard/#{params[:results]}" %>
<% else %>
  <%= render "decidim/elections/admin/dashboard/questions_with_results" %>
<% end %>
